{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es2016","dom"],
      "module": "commonjs",
      "declaration": true,
      "outDir": "./lib",
      "strict": true,
      "strictNullChecks":false
    },
    "include": ["src/main/js"],
    "exclude": ["node_modules", "**/__tests__/*"],
    "paths": {
      "@app/*" : ["main/js/*"]
    },
    "jsdoc": "jsdoc/out"
  }